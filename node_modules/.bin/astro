#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/terryyan/Documents/GitHub/ccat-atlas/node_modules/.pnpm/astro@5.17.0_@netlify+blobs@10.5.0_@types+node@24.8.1_idb-keyval@6.2.2_jiti@2.6.1_light_62e8a205b77b5430ceef3338128a65fb/node_modules/astro/node_modules:/Users/terryyan/Documents/GitHub/ccat-atlas/node_modules/.pnpm/astro@5.17.0_@netlify+blobs@10.5.0_@types+node@24.8.1_idb-keyval@6.2.2_jiti@2.6.1_light_62e8a205b77b5430ceef3338128a65fb/node_modules:/Users/terryyan/Documents/GitHub/ccat-atlas/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/terryyan/Documents/GitHub/ccat-atlas/node_modules/.pnpm/astro@5.17.0_@netlify+blobs@10.5.0_@types+node@24.8.1_idb-keyval@6.2.2_jiti@2.6.1_light_62e8a205b77b5430ceef3338128a65fb/node_modules/astro/node_modules:/Users/terryyan/Documents/GitHub/ccat-atlas/node_modules/.pnpm/astro@5.17.0_@netlify+blobs@10.5.0_@types+node@24.8.1_idb-keyval@6.2.2_jiti@2.6.1_light_62e8a205b77b5430ceef3338128a65fb/node_modules:/Users/terryyan/Documents/GitHub/ccat-atlas/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/astro@5.17.0_@netlify+blobs@10.5.0_@types+node@24.8.1_idb-keyval@6.2.2_jiti@2.6.1_light_62e8a205b77b5430ceef3338128a65fb/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../.pnpm/astro@5.17.0_@netlify+blobs@10.5.0_@types+node@24.8.1_idb-keyval@6.2.2_jiti@2.6.1_light_62e8a205b77b5430ceef3338128a65fb/node_modules/astro/astro.js" "$@"
fi
