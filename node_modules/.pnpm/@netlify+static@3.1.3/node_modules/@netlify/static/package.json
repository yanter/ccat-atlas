{
  "name": "@netlify/static",
  "version": "3.1.3",
  "description": "TypeScript implementtion of Netlify's static file serving logic",
  "type": "module",
  "engines": {
    "node": ">=20.6.1"
  },
  "main": "./dist/main.js",
  "exports": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup-node",
    "prepack": "npm run build",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:ci": "npm run build && vitest run",
    "dev": "tsup-node --watch",
    "publint": "npx -y publint --strict"
  },
  "keywords": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/netlify/primitives.git",
    "directory": "packages/static"
  },
  "bugs": {
    "url": "https://github.com/netlify/primitives/issues"
  },
  "author": "Netlify Inc.",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@netlify/dev-utils": "^4.3.3",
    "@types/mime-types": "^2.1.4",
    "tsup": "^8.5.0",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "mime-types": "^3.0.0"
  }
}
