  },
  "bugs": {
    "url": "https://github.com/netlify/primitives/issues"
  },
  "author": "Netlify Inc.",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "9.0.10",
    "tsup": "^8.5.0",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "@netlify/dev-utils": "4.3.3",
    "@netlify/redirect-parser": "^15.0.3",
    "cookie": "^1.0.2",
    "jsonwebtoken": "9.0.2",
    "netlify-redirector": "^0.5.0"
  }
